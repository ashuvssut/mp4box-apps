{
  "name": "transcode-plugin",
  "description": "mp4box-player plugin to enable transcoding of video chunks",
  "repository": "https://github.com/ashuvssut/mp4box-apps/",
  "author": "ashuvssut (ashu.khanduala@gmail.com)",
  "license": "MIT",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "dev": "tsup ./src/index.ts --watch --format cjs,esm --dts",
    "release": "yarn build",
    "lint": "tsc"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/util": "^0.12.1",
    "nanoid": "^5.0.6"
  },
  "devDependencies": {
    "tsup": "^6.5.0",
    "typescript": "^5.3.3"
  }
}
